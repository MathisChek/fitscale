<div class="progress">
  <div class="progress-bar bg-success" role="progressbar" style="width: <%= workout.avg_end * 10 %>%" aria-valuenow="<%= workout.avg_end * 10 %>" aria-valuemin="0" aria-valuemax="100"></div>
</div>
<div class="progress">
  <div class="progress-bar bg-danger" role="progressbar" style="width: <%= workout.avg_str * 10 %>%" aria-valuenow="<%= workout.avg_str * 10 %>" aria-valuemin="0" aria-valuemax="100"></div>
</div>
<div class="progress">
  <div class="progress-bar bg-warning" role="progressbar" style="width: <%= workout.avg_dex * 10 %>%" aria-valuenow="<%= workout.avg_dex * 10 %>" aria-valuemin="0" aria-valuemax="100"></div>
</div>
